{
  "ConnectionStrings": {
    "DefaultConnection": "ENC:Server=localhost;Database=mydb;User Id=myUsername;Password=myPassword;",
    "ReadOnlyConnection": "ENC:Server=readonly.example.com;Database=mydb;User Id=reader;Password=readerPass;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {
    "ApplicationName": "MyDotNetApp",
    "Version": "1.0.0",
    "ApiKey": "ENC:your-api-key-here",
    "Features": {
      "EnableCache": true,
      "MaxConcurrentRequests": 100,
      "RetryPolicy": {
        "MaxRetries": 3,
        "DelaySeconds": 5
      }
    }
  },
  "AllowedHosts": "*"
} 